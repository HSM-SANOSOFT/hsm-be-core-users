    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "typescript-eslint": "^8.26.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@nestjs/config": "^4.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.11",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
